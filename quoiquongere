Conversions :

's'		String						->	ft_getstr
'S'		Wchart *					->	ft_getsstr		(undone)
'p'		Pointeur					->	ft_getptr
'd'		Int							->	ft_getnbr
'D'		Long Int					->	ft_getlnbr
'i'		Int							->	ft_getnbr
'o'		Unsigned Octal				->	ft_getoctal
'O'		Unsigned Long Octal			->	ft_getloctal
'u'		Unsigned Int				->	ft_getunbr
'U'		Unsigned Long Int			->	ft_getlunbr
'x'		Unsigned Hexa				->	ft_gethexa
'X'		Unsigned Hexa				->	ft_getlhexa
'c'		Unsigned Char				->	ft_getchar
'C'		Win_t						->	ft_getwchar		(undone)

Flags :

'#'		Convert to another type 
'0'		Fill with '0'
'-'		Align at left
'+'		Put a '+' if positive
' '		Put a ' ' if positive

'hh'	Char
'h'		Short
'l'		Long
'll'	Long Long
'j'		Intmax_t || Uint_max
'z'		Size_t


